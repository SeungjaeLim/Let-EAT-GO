<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuCrawling">

    <Button
        android:id="@+id/button_kama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:background="@drawable/menubutton"
        android:text="카이마루"
        app:layout_constraintBottom_toTopOf="@+id/button_west"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_west"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/menubutton"
        android:text="서맛골"
        app:layout_constraintBottom_toTopOf="@+id/button_east"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_kama"
        app:layout_constraintVertical_bias="0.21"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_east"
        android:layout_width="wrap_content"
        android:layout_height="119dp"
        android:layout_marginBottom="180dp"
        android:background="@drawable/menubutton"
        android:text="동맛골"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_west"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>